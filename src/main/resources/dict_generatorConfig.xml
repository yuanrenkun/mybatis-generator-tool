<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<!-- <classPathEntry location="C:\mywork\maven\maven-repos\postgresql\postgresql\9.1-901-1.jdbc4\postgresql-9.1-901-1.jdbc4.jar" 
		/> -->
	<!-- 如果驱动在classpath下，例如通过maven依赖进来，则可以不在这里指定驱动位置 -->
	<classPathEntry
		location="D:\maven-repository\aliyun\mysql\mysql-connector-java\5.1.29\mysql-connector-java-5.1.29.jar" />
	<context id="context1" targetRuntime="MyBatis3"
		introspectedColumnImpl="com.generator.plugin.MyIntrospectedColumn">
		<property name="javaFileEncoding" value="UTF-8" />
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<plugin type="org.mybatis.generator.plugins.RenameExampleClassPlugin">
			<property name="searchString" value="Example$" />
			<property name="replaceString" value="Condition" />
		</plugin>
		<plugin type="com.generator.plugin.RenameExampleAndPrimaryKeyPlugin" />
		<plugin type="com.generator.plugin.MysqlPagingPlugin" />
		<plugin type="com.generator.plugin.TinyintToIntegerPlugin" />
		<plugin type="com.generator.plugin.CommentPlugin">
			<!-- 制定java bean的作者，默认情况下会读取计算机当前用户作为作者 -->
			<!--<property name="modelClassAuthor" value="xianwen.tan" /> -->
		</plugin>

		<commentGenerator>
			<!-- 不生成原生的注解信息 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://192.168.253.201:3306/new_krplus" userId="krplus"
			password="krplus" />

		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>

		<javaModelGenerator targetPackage="com.krplus.common.dictionary.modelxxx"
			targetProject="krplus-common-dictionary">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>

		<sqlMapGenerator targetPackage="com.krplus.common.dictionary.mapperxxx"
			targetProject="krplus-common-dictionary" />

		<javaClientGenerator targetPackage="com.krplus.common.dictionary.mapperxxx"
			targetProject="krplus-common-dictionary" type="XMLMAPPER" />


		<table tableName="dict_area" domainObjectName="DictArea">
			<columnOverride column="feature">
				<property name="forceInBlobModel" value="true" />
			</columnOverride>
			<columnOverride column="code">
				<property name="forceInBlobModel" value="true" />
			</columnOverride>
		</table>
		<!-- <table tableName="dict_school" domainObjectName="DictSchool"> <generatedKey 
			column="id" identity="true" sqlStatement="mysql" /> </table> -->

	</context>
</generatorConfiguration>